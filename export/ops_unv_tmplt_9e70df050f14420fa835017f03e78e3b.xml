# #!/usr/local/bin/python2.7
###################################################
#
#    Name: s3_boto_filemonitor.py
#
#    Purpose: Check for a file in a bucket 
#             All message not related to the S3 CLI
#             command are written to standard err.
#
#
#    Prerequisites:
#     - Universal Agent for Windows/Linux
#     - Python 2.7.x
#     - Requires the Python modules
#     - datetime, boto3 , botocore, argparse, sys
#     - Note: module can be added using pip
#     - e.g. use: pip install boto3
#
#     Version History:
#
#     1.0  Nils Buer 2017-11-10    using SB Code standards
#     2.0  Nils Buer 2018-04-04 logging and credentials
#          
####################################################
version = "2.0"

# Used modules

import re
import glob
import sys, os
import datetime, time
import logging
import boto3, botocore
import argparse

# -- loglevel settings INFO,DEBUG, WARNING, ERROR, CRITICAL

logging.basicConfig(level=logging.${ops_s3_loglevel}, format=' %(asctime)s - %(levelname)s - %(message)s')

def main(): 
    SetVariables()                                # -- Set Variables from Universal Task Form or Command Line Input
    Check_File_on_S3()                            # -- retrieves the session token 


# -- Set Variables from Universal Task Form or Command Line Input
def SetVariables():
    parser=argparse.ArgumentParser(description='Purpose : List the content of a bucket')
    # ## --&gt; AWS Connection Settings:
    parser.add_argument("--useproxy", default="${ops_s3_useproxy}")
    parser.add_argument("--port", default="${ops_s3_port}")
    parser.add_argument("--proxy", default="${ops_s3_proxy}")
    parser.add_argument("--proxyuser", default="${_credentialUser('${ops_s3_proxycred}')}")  
    parser.add_argument("--proxypwd", default="${_credentialPwd('${ops_s3_proxycred}')}")
    parser.add_argument("--AWS_ACCESS_KEY_ID", default="${_credentialPwd('${ops_s3_aws_access_key_id}')}")  
    parser.add_argument("--AWS_SECRET_ACCESS_KEY", default="${_credentialPwd('${ops_s3_aws_secret_access_key}')}")
    parser.add_argument("--AWS_DEFAULT_REGION", default="${_credentialPwd('${ops_s3_aws_default_region}')}")

    
    # ## --&gt;S3 CLI specific Settings
    parser.add_argument("--interval", default="${ops_s3_interval}")
    parser.add_argument("--s3key", default="${ops_s3_s3key}")
    parser.add_argument("--bucketname", default="${ops_s3_bucketname}")
    
    
    # ## -- Printing out Version and Arguments to standard errro log file
    global args
    args=parser.parse_args()
    logging.info("Executing version " + version + " with the following paramaters")
    logging.info(args)
# --
# ## --&gt; Functions Go Here

def make_ressource_connection():
 from botocore.config import Config
 logging.debug('Establishing connection')
 if int(args.useproxy) == 1 :  
 # connection via Proxy
  logging.info('S3 Connection via Proxy: %s' % (args.proxy) )
  os.environ['https_proxy'] = "https://%s:%s@%s:%s" % (args.proxyuser, args.proxypwd, args.proxy, args.port)
  s3client = boto3.resource(
     's3',
     aws_access_key_id = args.AWS_ACCESS_KEY_ID,
     aws_secret_access_key=args.AWS_SECRET_ACCESS_KEY,
     region_name=args.AWS_DEFAULT_REGION   
  ) 
 else:
  # connection without Proxy
  logging.info('Direct S3 Connection without Proxy')
  s3client = boto3.resource(
     's3',
     aws_access_key_id = args.AWS_ACCESS_KEY_ID,
     aws_secret_access_key=args.AWS_SECRET_ACCESS_KEY,
     region_name=args.AWS_DEFAULT_REGION 
  )
 return s3client 
# -- make_ressource_connection()

def Check_File_on_S3():

 # check for file in S3 bucket
 s3client = make_ressource_connection()
 found = False # allow for at least one iteration
 while not found:
   try:
      s3client.Object('%s'  % (args.bucketname),'%s' % (args.s3key) ).load()
   except botocore.exceptions.ClientError as e:
      if e.response['Error']['Code'] == "404":
         found=False
         time.sleep( float(args.interval) )
      else:
         raise
   else:
       logging.info(' Object: %s found in bucket: %s' % (args.s3key, args.bucketname))
       found=True


# -- Execute
